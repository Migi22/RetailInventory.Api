				{
					"name": "Login as Owner1",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"200 OK\", () => pm.response.to.have.status(200));",
									"",
									"const json = pm.response.json();",
									"pm.collectionVariables.set(\"ownerToken\", json.token);",
									"",
									"// helper: base64 decode in Postman/Node-safe way",
									"function base64Decode(str) {",
									"    return Buffer.from(str, 'base64').toString('binary');",
									"}",
									"",
									"// decode JWT to get StoreId",
									"function parseJwt(t){",
									"  const p = t.split('.')[1];",
									"  const b = p.replace(/-/g,'+').replace(/_/g,'/');",
									"  return JSON.parse(decodeURIComponent(base64Decode(b).split('').map(c =>",
									"    '%' + ('00'+c.charCodeAt(0).toString(16)).slice(-2)",
									"  ).join('')));",
									"}",
									"",
									"const payload = parseJwt(json.token);",
									"if (payload.StoreId) {",
									"  pm.collectionVariables.set(\"ownerStoreId\", String(payload.StoreId));",
									"}",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
